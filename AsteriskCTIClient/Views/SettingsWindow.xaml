<Window x:Class="AsteriskCTIClient.Views.SettingsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SettingsWindow" Height="191" Width="397" ShowInTaskbar="False">
  <Grid Height="155">


    <TextBox Text="{Binding ExtensionNumber}" Height="32" HorizontalAlignment="Left" Margin="116,44,0,78" Name="txExt"
             VerticalAlignment="Center" Width="110" FontSize="14" />
    <Label BorderThickness="1" Content="This Extension :" Height="32" HorizontalAlignment="Left" Name="label1"
           VerticalAlignment="Top" Width="110" FontSize="14" Margin="9,44,0,0" />
    <Label BorderThickness="1" Content="Server :" FontSize="14" Height="32" HorizontalAlignment="Left"
           Margin="10,6,0,0" Name="label2" VerticalAlignment="Top" Width="79" />
    <TextBox Text="{Binding ServerAddress}" FontSize="14" Height="32" HorizontalAlignment="Left" Margin="116,6,0,117"
             Name="txServer" VerticalAlignment="Center" Width="110" />
    <TextBlock TextWrapping="Wrap" Text="{Binding ConnectionStatus}" Margin="9,84,0,0" VerticalAlignment="Top"
               Height="66" HorizontalAlignment="Left" Width="358" />
  </Grid>
</Window>