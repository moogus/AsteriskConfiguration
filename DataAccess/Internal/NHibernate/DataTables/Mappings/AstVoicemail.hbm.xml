<?xml version="1.0" encoding="utf-8"?>

<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="DataAccess"
                   namespace="DataAccess.Internal.NHibernate.DataTables.Classes">

  
  <class name ="AstVoicemail" table="ast_voicemail">
    <id name="Id" column="uniqueid">
      <generator class="native"/>
    </id>
    <property name="Context" column="context"/>
    <property name="Mailbox" column="mailbox"/>
    <property name="Password" column="password" />
    <property name="FullName" column="fullname" />
    <property name="Email" column="email"/>
    <property name="MaxSecs" column="maxsecs"/>
    <property name="MinSecs" column="minsecs"/>
    <property name="MaxMessages" column="maxmsg"/>
    <property name="HeldMaxMessages" column="fu_heldMaxMsg"/>
    <property name="Attach" column="attach"/>
  </class>
</hibernate-mapping>